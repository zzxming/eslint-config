{
  "name": "@zzxming/eslint-config",
  "type": "module",
  "version": "0.5.1",
  "packageManager": "pnpm@10.12.4",
  "description": "Eslint config for zzxming",
  "author": "zzxming",
  "license": "MIT",
  "homepage": "https://github.com/zzxming/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "uvs": "./bin/update-vscode-settings.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "pnpm type && unbuild",
    "dev": "npx @eslint/config-inspector --config eslint.config.js",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build --base /eslint-config",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "type": "tsx ./scripts/typegen.ts"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.48.5",
    "@prettier/plugin-xml": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@unocss/eslint-plugin": "^66.0.0",
    "@vitest/eslint-plugin": "^1.1.43",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-vue": "^10.1.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "vue-eslint-parser": "^10.1.3"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@eslint/markdown": "^7.0.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@unocss/eslint-plugin": "^66.3.2",
    "@vitest/eslint-plugin": "^1.3.4",
    "consola": "^3.4.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-de-morgan": "^1.3.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^51.3.4",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-perfectionist": "^4.15.0",
