{
  "name": "@zzxming/eslint-config",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@9.9.0",
  "description": "Eslint config for zzxming",
  "author": "zzxming",
  "license": "MIT",
  "homepage": "https://github.com/zzxming/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "uvs": "./bin/update-vscode-settings.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "npx @eslint/config-inspector --config eslint.config.js",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build --base /eslint-config",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.11.0"
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "consola": "^3.2.3",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-plugin-antfu": "^2.3.3",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import-x": "^4.3.0",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.9.0",
    "@eslint/config-inspector": "^0.5.1",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^20.14.9",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/eslint-plugin": "^1.1.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.6.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.7",
    "local-pkg": "^0.5.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
