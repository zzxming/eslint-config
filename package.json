{
  "name": "@zzxming/eslint-config",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@9.15.6",
  "description": "Eslint config for zzxming",
  "author": "zzxming",
  "license": "MIT",
  "homepage": "https://github.com/zzxming/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "uvs": "./bin/update-vscode-settings.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "npx @eslint/config-inspector --config eslint.config.js",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build --base /eslint-config",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.17.0"
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "consola": "^3.4.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-format": "^0.1.3",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "jsonc-eslint-parser": "^2.4.0",
    "prompts": "^2.4.2",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.30.2",
    "@eslint/config-inspector": "^1.0.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/node": "^22.13.9",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@unocss/eslint-plugin": "^65.5.0",
    "@vitest/eslint-plugin": "^1.1.36",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "lint-staged": "^15.4.3",
    "local-pkg": "^1.1.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^2.0.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
