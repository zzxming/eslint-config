{
  "name": "@zzxming/eslint-config",
  "description": "zzxming's eslint config",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.14.3",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "eslint-config"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "author": "zzxming",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.9",
    "eslint": "^9.3.0",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0"
  }
}
